{
  "name": "influx",
  "version": "4.2.0",
  "description": "InfluxDB Client",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "tsc -p src/",
    "lint": "tslint src/**/*.ts",
    "doc": "esdoc -c esdoc.json",
    "test": "mocha -R spec -r test/unit/_setup.js test/unit/*.test.js",
    "integrate": "mocha -R spec test/integration/*.test.js && standard",
    "cover": "istanbul cover _mocha -- -R spec -r test/unit/_setup.js",
    "travis-test": "istanbul cover _mocha --report lcovonly -- -R spec -r test/unit/_setup.js test/unit/*.test.js && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-influx/node-influx.git"
  },
  "keywords": [
    "influx",
    "influxdb",
    "time",
    "series",
    "client",
    "db"
  ],
  "contributors": [
    "Ben Evans <ben@bensbit.co.uk> (http://bensbit.co.uk)",
    "Connor Peet <connor@peet.io>",
    "Steffen Konerow <steffen@nrg-media.de> (http://www.nrg-media.de)"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "before",
      "after",
      "expect"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.11.1",
    "request": "2.x"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.1",
    "esdoc": "^0.4.7",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "standard": "^7.0.1",
    "tslint": "^3.11.0",
    "typedoc": "^0.4.3",
    "typescript": "^1.8.10"
  }
}
